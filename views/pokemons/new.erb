<h2>New Pokemon</h2>

<form action="/pokemons" method="post" >
  <label>Name:</label>
  <input name="pokemon[name]" type="text">

  <label>poke_type:</label>
  <input name="pokemon[poke_type]" type="text">

  <label>CP:</label>
  <input name="pokemon[cp]" type="text">

  <label>img_url:</label>
  <input name="pokemon[img_url]" type="text">

  <input type="submit" value="Create">
</form>
